class Solution {
    public int[] getConcatenation(int[] nums) {
       int n[] = new int[nums.length*2];
        for(int i=0; i<n.length; i++)
        {
            if(i<(n.length)/2)
            {
                n[i]=nums[i];
            }
            else
            {
                n[i]=nums[i-nums.length];
            }
        }
        return(n);
    }
}
